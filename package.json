{
  "name": "cypress-automation-tests",
  "version": "1.0.0",
  "description": "Repo comprised of automation tests written for Cypress",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:headless": "cypress run --browser chrome --headless --spec cypress/e2e/**/*.cy.js",
    "cy:dockerheadless": "cypress run --browser chromium --headless --spec cypress/e2e/**/*.cy.js",
    "lint:check": "eslint . && prettier --check.",
    "prettier:write": "prettier . --write",
    "prepare": "husky"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^13.8.0",
    "cypress-recurse": "^1.35.3",
    "eslint": "^8.57.0",
    "eslint-plugin-chai-friendly": "^0.7.4",
    "eslint-plugin-cypress": "^3.0.0",
    "eslint-plugin-mocha": "^10.4.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5"
  }
}
